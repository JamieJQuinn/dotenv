# User bin
set PATH $PATH /home/vagrant/bin

# fap devops
set PATH $PATH /home/vagrant/prog/fap/devops-scripts

function fish_user_key_bindings
  bind \cf 'fg %'
end

alias gc 'git checkout'
alias gs 'git status'
alias gd 'git diff'
alias gb 'git branch'

alias be 'bundle exec'

function a
	xdg-open $argv
end

# ASDF
source ~/.asdf/asdf.fish

# Direnv
eval (direnv hook fish)

# hub
eval (hub alias -s)

